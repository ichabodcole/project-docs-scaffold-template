{
  "name": "project-docs-scaffold-template",
  "version": "1.3.0",
  "description": "Cookiecutter template for scaffolding project documentation structure",
  "private": true,
  "scripts": {
    "format": "prettier --write \"**/*.md\"",
    "format:check": "prettier --check \"**/*.md\"",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.2.5"
  },
  "keywords": [
    "cookiecutter",
    "template",
    "documentation",
    "scaffold"
  ],
  "author": "Cole Reed",
  "license": "MIT"
}
